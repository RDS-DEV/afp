<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <typeAliases>
        <package name="gov.hhs.cms.afs.domain" />
    </typeAliases>

    <environments default="development">
        <environment id="aes">
            <transactionManager type="JDBC" />
            <dataSource type="POOLED">
                <property name="driver" value="com.mysql.jdbc.Driver" />
                <property name="url" value="jdbc:mysql://localhost:3306/aes?verifyServerCertificate=false&amp;useSSL=true" />
                <property name="username" value="prpadmin" />
                <property name="password" value="abcd1234" />


            </dataSource>
        </environment>


        <environment id="cps">
            <transactionManager type="JDBC" />
            <dataSource type="POOLED">
                <property name="driver" value="com.mysql.jdbc.Driver" />
                <property name="url" value="jdbc:mysql://localhost:3306/cps?verifyServerCertificate=false&amp;useSSL=true" />
                <property name="username" value="prpadmin" />
                <property name="password" value="abcd1234" />


            </dataSource>
        </environment>
    </environments>


    <mappers>
        <mapper resource="final-mapper.xml" />
        <mapper resource="mybatis-config.xml" />
        <mapper resource="mybatiscps-config.xml" />
        <mapper resource="result-mapper.xml" />
    </mappers>
</configuration>